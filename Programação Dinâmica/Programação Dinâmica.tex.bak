\section{Programação Dinâmica em Intervalos}

\begin{frame}{Maior substring palindrome}

\begin{itemize}
\item Problema: Dado uma string, encontre o tamanho da maior substring que é palindrome.
\item Por exemplo, se a string é "desproggorpdes", a maior string palindrome é "proggorp".
\end{itemize}

\end{frame}

\begin{frame}{Maior substring palindrome}

\begin{itemize}
    \item Defina os subproblemas

    \begin{itemize}
        \item Seja $D_{ij} =
        \begin{cases}
        1, & \mbox{ se s[i \ldots j] é palindrome}\\
        0, & \mbox{ caso contrário}
        \end{cases}
        $
    \end{itemize}



    \item Recorrência

    \begin{itemize}
    \item Seja $D_{ij} = D_{i-1,j-1} \&\& s[i] == s[j]$
    \end{itemize}

    \item Resolva os casos bases:

    \begin{itemize}
    \item $D_{ii} = 1$
    \item $D_{i,i+1} = s[i]==s[i+1]$
    \end{itemize}

    \item A tabela é preenchida diagonalmente.

\end{itemize}


\end{frame}

